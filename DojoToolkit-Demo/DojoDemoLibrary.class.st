Class {
	#name : #DojoDemoLibrary,
	#superclass : #WAFileLibrary,
	#category : 'DojoToolkit-Demo'
}

{ #category : #accessing }
DojoDemoLibrary >> demoCss [

	^ '

body {
	font-family: sans-serif;
}

span.selection {
	font-weight: bold;
}

span.selector {
	font-weight: bold;
}

em {
	font-style: normal;
	color: #4488FF;
}

a {
	display: inline-block;
	color: #4488FF;
	text-decoration: none;
	border-bottom-style: dotted;
	border-bottom-width: 1px;
	border-bottom-color: #888888;
}

a:hover {
	border-bottom-style: solid;
	border-bottom-width: 1px;
	border-bottom-color: #4488FF;
}

h2.navigation {
	border-style: none;
	color: #333333;
}

h2 {
	display: block;
	border-bottom-style: solid;
	border-bottom-width: 1px;
}

h3 {
	font-weight: normal;
	display: block;
	border-bottom-style: dotted;
	border-bottom-width: 1px;
}

ul.navigation {
	list-style-type: none;
	magin: 0px;
	padding: 0px;
}

li.navigation {
	padding-left: 10px;
	margin: 5px;
	height: 1.5em;
}

a.navigation {
	color: black;
	text-decoration: none;
	display: block;
}

div.canvas {
	margin: 20px;
	width: 900px;
}

div.navigation {
	left: 50px;
	width: 200px; 
	position: absolute;
	background-color: #F0F8FF;
	padding: 5px;
	margin: 5px;
}

div.demo {
	left: 280px; 
	width: 700px; 
	position: absolute;
	padding: 5px;
	margin: 5px;
}

div.code {
	background-color: #F8F8F8;
	padding: 5px;
	margin-top: 10px;
	margin-bottom: 10px;
	border-width: 1px;
	border-style: solid;
	border-color: #888888;
}

img.treeOpened {
	width: 16px;
	height: 16px;
	background: url(folderopen.png) no-repeat scroll 0 0 transparent;
}

img.treeClosed {
	width: 16px;
	height: 16px;
	background: url(folder.png) no-repeat scroll 0 0 transparent;
}

img.treeLeaf {
	width: 16px;
	height: 16px;
	background: url(textxgeneric.png) no-repeat scroll 0 0 transparent;
}
'
]

